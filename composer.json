{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "askedio/laravel-soft-cascade": ">0.1",
        "doctrine/dbal": "^3.1",
        "fakerphp/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "kolydart/common": "dev-master",
        "kolydart/laravel": "dev-master",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^4.0",
        "laravelcollective/html": "^6.2",
        "sentry/sentry-laravel": "^3.0",
        "spatie/laravel-activitylog": "^4.0.0",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "barryvdh/laravel-ide-helper": "*",
        "beyondcode/laravel-query-detector": "^2.1",
        "brianium/paratest": "^6.3",
        "jasonmccreary/laravel-test-assertions": "^2.1",
        "laravel/boost": "^1.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-ignition": "^2.0",
        "symfony/thanks": "^1.2"
    },
    "autoload": {
        "classmap": [],
        "exclude-from-classmap": [],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "if [ $COMPOSER_DEV_MODE -eq 1 ] && [ ! -L vendor/kolydart/laravel ] ; then rm -rf vendor/kolydart/laravel && ln -s ../../../kolydart-laravel vendor/kolydart/laravel; fi",
            "if [ $COMPOSER_DEV_MODE -eq 0 ] && [ -L vendor/kolydart/laravel ] ; then unlink vendor/kolydart/laravel && composer update kolydart/laravel --no-dev; fi",
            "if [ $COMPOSER_DEV_MODE -eq 1 ] && [ ! -L vendor/kolydart/common ] ; then rm -rf vendor/kolydart/common && ln -s ../../../kolydart-common vendor/kolydart/common; fi",
            "if [ $COMPOSER_DEV_MODE -eq 0 ] && [ -L vendor/kolydart/common ] ; then unlink vendor/kolydart/common && composer update kolydart/common --no-dev; fi",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "[ $COMPOSER_DEV_MODE -eq 0 ] || php artisan ide-helper:generate",
            "[ $COMPOSER_DEV_MODE -eq 0 ] || php artisan ide-helper:models --nowrite"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "symfony/thanks": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
